{
  "name": "npm-react-app-boilerplate",
  "version": "1.0.0",
  "description": "The boilerplate to build React application with live-editing",
  "scripts": {
    "start": "node server.js",
    "compile": "babel --presets es2015,stage-0,react -d lib/ src/",
    "lint": "eslint src",
    "test": "test",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcogazzola/npm-react-app-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "marco.gazzola <reactjs@marcogazzola.com> (http://github.com/marcogazzola)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcogazzola/npm-react-app-boilerplate"
  },
  "homepage": "https://github.com/marcogazzola/npm-react-app-boilerplate",
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "^6.0.20",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  }
